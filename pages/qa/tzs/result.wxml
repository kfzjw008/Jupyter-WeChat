<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">

            <view class="weui-msg__title">竞赛完成</view>
 <view class="weui-flex">
            <view class="weui-flex__item" style="" >
                   <view class="weui-msg__desc">用户昵称 </view>
               <view class="weui-msg__desc">限定时间</view>
            <view class="weui-msg__desc">题目总数</view>
             <view class="weui-msg__desc">正确总数</view>
              <view class="weui-msg__desc">最终得分</view>

   
            </view>
           
             <view class="weui-flex__item"  style="">
               <view class="weui-msg__desc"><open-data type="userNickName" lang="zh_CN"></open-data> </view>
               <view class="weui-msg__desc">{{sj}}秒</view>
            <view class="weui-msg__desc">{{cstimu}}题</view>
             <view class="weui-msg__desc">{{csrighttimu}}题</view>
              <view class="weui-msg__desc">{{csscore}}分</view>

             </view>
        </view> 
           
                <view class="weui-msg__desc">最后一步，输入学校和邮箱，用于证书领取</view>
        </view>


        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">

                    <view class="weui-label" style='font-size:15px'>学校</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" bindinput="jd" style='font-size:15px' placeholder="非在校生请填无" />
                </view>
            </view>
   <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">

                    <view class="weui-label" style='font-size:15px'>邮箱</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" bindinput="wd" style='font-size:15px' placeholder="邮箱用于证书发放" />
                </view>
            </view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="btn1" hover-class="btn-hover" bindtap="gotoPage6">上传竞赛记录</button>
            </view>
        </view>
      
    </view>
</view>