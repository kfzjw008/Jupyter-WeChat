<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">测试完成</view>
            <view class="weui-msg__desc">题目总数：{{cstimu}}</view>
             <view class="weui-msg__desc">正确总数：{{csrighttimu}}</view>
              <view class="weui-msg__desc">最终得分：{{csscore}}</view>
                <view class="weui-msg__desc">是否确定上传测试记录？</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" style='background: #3763BA;'  type="primary" bindtap="gotoPage6">上传记录</button>
                <button class="weui-btn" type="default"  bindtap="gotoPage2">取消上传</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="" class="weui-footer__link">在线测试模块V1.0测试版</navigator>
                </view>
                <view class="weui-footer__text">题目来源：自命题和部分网络资料</view>
            </view>
        </view>
    </view>
</view>